{
  "name": "@tar-erpedia/deep-pack",
  "version": "0.0.11",
  "description": "pack a package and pack it's dependencies",
  "main": "./lib/deep-pack.js",
  "bin": {
    "npm-deep-pack": "./bin/deep-pack.js"
  },
  "scripts": {
    "debug-test": "cd ./debug && nodemon ../src/deep-pack.ts \"tii\"",
    "build": "tsc -p .",
    "local": "sudo npm i -g && npm-deep-pack",
    "zpublish": "npm version patch --force && npm publish --access public",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tar-erpedia/deep-pack.git"
  },
  "keywords": [
    "npm",
    "pack",
    "package",
    "tgz",
    "tar-tgz"
  ],
  "author": "DoradSoft",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/tar-erpedia/deep-pack/issues"
  },
  "homepage": "https://github.com/tar-erpedia/deep-pack#readme",
  "dependencies": {
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "commander": "^7.0.0",
    "figlet": "^1.5.0",
    "fs": "0.0.1-security",
    "node-fetch": "^2.6.1",
    "npm-package-arg": "^8.1.0",
    "npm-package-json-loader": "^1.0.43",
    "path": "^0.12.7",
    "requirejs": "^2.3.6",
    "semver": "^7.3.4",
    "status-code-enum": "^1.0.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.33",
    "@types/clear": "^0.1.1",
    "@types/figlet": "^1.2.1",
    "@types/node": "^14.14.25",
    "@types/node-fetch": "^2.5.8",
    "@types/npm-package-arg": "^6.1.0",
    "@types/validate-npm-package-name": "^3.0.2",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
